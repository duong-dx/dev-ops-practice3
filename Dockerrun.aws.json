{
  "AWSEBDockerrunVersion": 2,
  "containerDefinition": [
    {
      "name": "client",
      "image": {
        "name": "duong1200798/multi-client",
        "update": true
      },
      "hostname": "react-client",
      "essential": false,
      "memory": 128
    },
    {
      "name": "server",
      "image": {
        "name": "duong1200798/multi-server",
        "update": true
      },
      "hostname": "api-server",
      "essential": false,
      "memory": 128
    },
    {
      "name": "worker",
      "image": {
        "name": "duong1200798/multi-worker",
        "update": true
      },
      "hostname": "worker",
      "essential": false,
      "memory": 128
    },
    {
      "name": "nginx",
      "image": {
        "name": "duong1200798/multi-nginx",
        "update": true
      },
      "hostname": "nginx",
      "essential": true,
      "portMapping": [
        {
          "hostPort": "80",
          "containerPort": "80"
        }
      ],
      "links": ["client", "server"],
      "memory": 128
    }
  ]
}